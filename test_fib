def test_fibonacci_sequence():
    # Test case 1: First 5 Fibonacci numbers
    assert fibonacci_sequence(5) == [0, 1, 1, 2, 3], "Test case 1 failed"

    # Test case 2: No terms (empty sequence)
    assert fibonacci_sequence(0) == [], "Test case 2 failed"

    # Test case 3: First 1 Fibonacci number
    assert fibonacci_sequence(1) == [0], "Test case 3 failed"


